{% extends 'base.html.twig' %}

{% block title %}Support{% endblock %}

{% block body %}
    <h1>Support</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ support.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ support.title }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ support.content }}</td>
            </tr>
            <tr>
                <th>IsParent</th>
                <td>{{ support.isParent ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Parent</th>
                <td>{{ support.parent }}</td>
            </tr>
            <tr>
                <th>Rank</th>
                <td>{{ support.rank }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('support_index') }}">back to list</a>

    <a href="{{ path('support_edit', {'id': support.id}) }}">edit</a>

    {{ include('support/_delete_form.html.twig') }}
{% endblock %}
